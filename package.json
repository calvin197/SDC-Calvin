  "scripts": {
    "mongoimport": "mongoimport --db morelooks --collection products --file ./database/mongoose/dummyData.json --jsonArray --maintainInsertionOrder --numInsertionWorkers 5",
    "seeder": "pm2 start database/seed.js -i max --watch",
    "stop-seeder": "pm2 stop database/seed.js -i 0",
    "monitor": "pm2 monit",
    "seed-mongo": "node database/seed.js",
    "start": "nodemon server/index.js",
    "start-pm2": "pm2 start server/index.js -i -1",
    "build": "webpack -d --watch",
    "write-data-json": "node database/dummyWriterJSON.js",
    "write-data-csv": "node database/dummyWriterCSV.js",
    "artillery": "artillery run hello.yml",
    "loadtest": "loadtest -t 10 -c 50 --rps 1000 http://localhost:3003/morelooks/api/purses;",
    "redis": "redis-server",
    "mongo": "mongod --config /usr/local/etc/mongod.conf"
  },
  "dependencies": {
